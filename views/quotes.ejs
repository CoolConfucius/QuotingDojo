<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>QuotingDojo</title>
  <style>
    form label, form input {
      display: block;
    }
  </style>
</head>
<body>
  <h2>Here are the awesome quotes!</h2>
  <div class="container">

    <% quotes.forEach(function(element, index){ %>
      <p>" <%= element.quote %> "</p>
      <p>-<%= element.name %> at time</p>
    <% }) %>
    
    <br>
    <a href="/">Home</a>
  </div>
</body>
</html>